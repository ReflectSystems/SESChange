<%
   data_points       = @ses.statistics.result.sort_by{|x| x["Timestamp"]}
   dates             = data_points.map{|x|x["Timestamp"]}
   dates             = dates.map do |date_string|
                          datetime = DateTime.parse(date_string)
                          "#{datetime.month}-#{datetime.day}"
                       end.uniq
   bounces = data_points.map{|x|x["Bounces"]}

%>
<h2>Bounces: <%= data_points.map{|x|x["Bounces"]}.map(&:to_i).inject{|sum, x| sum+x} %> over last 2 weeks</h2>
<br/>
<br/>
<br/>

<img src="http://chart.apis.google.com/chart?chxl=0:|<%= dates.join("|") %>|1:|<%= bounces.min %>|<%= bounces.max %>&chxr=0,1,3&chxt=x,y&chs=600x300&cht=lc&chd=t:<%= bounces.join(",") %>&chg=25,50&chls=0.75,-1,-1|2,4,1&chm=o,FF9900,1,-2,8|b,3399CC44,0,1,0&chtt=Delivery+Attempts" width="600" height="300" alt="Delivery Attempts" />
